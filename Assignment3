  <!-- Unordered List of Fruits -->
  <ul>
    <li>Apple</li>
    <li>Banana</li>
    <li>Cherry</li>
    <li>Watermelon</li>
    <li>Grapes</li>
  </ul>

  <!-- Ordered List of Fruits -->
  <ol>
    <li>Apple</li>
    <li>Banana</li>
    <li>Cherry</li>
    <li>Watermelon</li>
    <li>Grapes</li>
  </ol>
